<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:air_v29_0="http://www.travelport.com/schema/air_v29_0" xmlns:hotel_v29_0="http://www.travelport.com/schema/hotel_v29_0" xmlns:common="http://www.travelport.com/schema/common_v29_0" xmlns:vehicle_v29_0="http://www.travelport.com/schema/vehicle_v29_0" xmlns="http://www.travelport.com/schema/universal_v29_0" xmlns:passive_v29_0="http://www.travelport.com/schema/passive_v29_0" xmlns:cruise_v29_0="http://www.travelport.com/schema/cruise_v29_0" xmlns:rail_v29_0="http://www.travelport.com/schema/rail_v29_0" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.travelport.com/schema/universal_v29_0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="../common_v29_0/Common.xsd" namespace="http://www.travelport.com/schema/common_v29_0" />
  <xs:import schemaLocation="../vehicle_v29_0/Vehicle.xsd" namespace="http://www.travelport.com/schema/vehicle_v29_0" />
  <xs:import schemaLocation="../air_v29_0/Air.xsd" namespace="http://www.travelport.com/schema/air_v29_0" />
  <xs:import schemaLocation="../hotel_v29_0/Hotel.xsd" namespace="http://www.travelport.com/schema/hotel_v29_0" />
  <xs:import schemaLocation="../passive_v29_0/Passive.xsd" namespace="http://www.travelport.com/schema/passive_v29_0" />
  <xs:import schemaLocation="../rail_v29_0/Rail.xsd" namespace="http://www.travelport.com/schema/rail_v29_0" />
  <xs:import schemaLocation="../cruise_v29_0/Cruise.xsd" namespace="http://www.travelport.com/schema/cruise_v29_0" />
  <xs:annotation>
    <xs:documentation>
            (c) Travelport GDS 2008 All rights reserved. Proprietary
            Information of Travelport GDS.
        </xs:documentation>
  </xs:annotation>
  <xs:element name="SupportedVersions">
    <xs:annotation>
      <xs:documentation>
                SOAP Header element that determines what version a
                particular request supports
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="urVersion" type="xs:string" use="optional" />
      <xs:attribute name="airVersion" type="xs:string" use="optional" />
      <xs:attribute name="hotelVersion" type="xs:string" use="optional" />
      <xs:attribute name="vehicleVersion" type="xs:string" use="optional" />
      <xs:attribute name="passiveVersion" type="xs:string" use="optional" />
      <xs:attribute name="railVersion" type="xs:string" use="optional" />
      <xs:attribute name="cruiseVersion" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>
  <xs:element name="UniversalRecord">
    <xs:annotation>
      <xs:documentation>
                Universal Record holds one or more provider reservations
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:LinkedUniversalRecord" />
        <xs:element minOccurs="0" maxOccurs="1" ref="common:Group" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:BookingTraveler" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:ServiceFeeInfo" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:OSI" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:ActionStatus" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ProviderReservationInfo" />
        <xs:choice>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="air_v29_0:AirReservation" />
        </xs:choice>
        <xs:choice>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="hotel_v29_0:HotelReservation" />
        </xs:choice>
        <xs:choice>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="vehicle_v29_0:VehicleReservation" />
        </xs:choice>
        <xs:choice>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="passive_v29_0:PassiveReservation" />
        </xs:choice>
        <xs:choice>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="rail_v29_0:RailReservation" />
        </xs:choice>
        <xs:choice>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="cruise_v29_0:CruiseReservation">
            <xs:annotation>
              <xs:documentation>The parent container for all cruise booking data. Supported Providers :1V</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:choice>
        <xs:choice>
          <xs:element minOccurs="0" maxOccurs="unbounded" ref="air_v29_0:EMDSummaryInfo">
            <xs:annotation>
              <xs:documentation>List of EMDs to be shown as part of UR. Supported providers are 1V/1G/1P/1J</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:ProviderARNKSegment" />
        <xs:element minOccurs="0" maxOccurs="1" ref="SegmentContinuityInfo" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:XMLRemark" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:GeneralRemark" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:AccountingRemark" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:UnassociatedRemark" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:Postscript" />
        <xs:element minOccurs="0" ref="common:AgencyInfo" />
        <xs:element minOccurs="0" ref="common:AppliedProfile" />
        <xs:element minOccurs="0" ref="common:AgencyContactInfo" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:CustomerID" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:CommissionRemark" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:ConsolidatorRemark" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="UnmaskedDataRemark" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:InvoiceRemark" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:ReviewBooking">
          <xs:annotation>
            <xs:documentation>Review Booking or Queue Minders is to add the reminders in the Provider Reservation along with the date time and Queue details. On the date time defined in reminders, the message along with the PNR goes to the desired Queue. </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:SSR">
          <xs:annotation>
            <xs:documentation>SSR's having no bookingTravelerRef need to add at providerReservation level outside bookingTraveler</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:InvoiceData" />
      </xs:sequence>
      <xs:attribute name="LocatorCode" type="common:typeLocatorCode" use="required">
        <xs:annotation>
          <xs:documentation>Unique Identifier of a Universal Record. If this is ViewOnly UR then Locator Code is '999999'.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SavedTripLocatorCode" type="common:typeLocatorCode" use="optional" />
      <xs:attribute name="LockReason" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>
                        The reason for which the reservation is
                        currently locked for modifications
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="CreateDate" type="xs:dateTime" use="optional">
        <xs:annotation>
          <xs:documentation>
                        The date and time that this reservation was
                        created.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Version" type="common:typeURVersion" use="optional" />
      <xs:attribute name="Status" type="typeURStatus" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="UnmaskedDataRemark">
    <xs:annotation>
      <xs:documentation>Security remark to a PNR .If a PCC is a Bridge-Branch/5-CP-Consolidator AND is also listed in this field,it can retrieve the PNR data UNMASKED and UNSUPPRESSED.PROVIDER SUPPORTED:JAL.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="11" ref="common:PseudoCityCode" />
      </xs:sequence>
      <xs:attribute name="Key" type="common:typeRef" use="optional">
        <xs:annotation>
          <xs:documentation>Key to be used for internal processing.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ProviderReservationInfoRef" type="common:typeRef" use="optional">
        <xs:annotation>
          <xs:documentation>Provider reservation reference key.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ProviderCode" type="common:typeProviderCode" use="optional">
        <xs:annotation>
          <xs:documentation>Contains the Provider Code of the provider for which this element is used</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attributeGroup ref="common:attrElementKeyResults" />
    </xs:complexType>
  </xs:element>
  <xs:element name="ProviderReservationInfo">
    <xs:annotation>
      <xs:documentation>
                Provider Reservation informations
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:choice minOccurs="0">
          <xs:element minOccurs="0" ref="ProviderReservationDetails" />
          <xs:element minOccurs="0" ref="ProviderReservationDisplayDetailsList" />
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="1" ref="ExternalReservationInfo" />
      </xs:sequence>
      <xs:attribute name="Key" type="common:typeRef" use="required">
        <xs:annotation>
          <xs:documentation>
                        Key value of the provider reservation
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required">
        <xs:annotation>
          <xs:documentation>
                        Contains the Provider Code of the entity housing
                        the actual reservation in the event this is a
                        passive one.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="LocatorCode" type="common:typeProviderLocatorCode" use="required">
        <xs:annotation>
          <xs:documentation>
                        Contains the Locator Code of the actual
                        reservation in the event this is a passive
                        reservation.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="CreateDate" type="xs:dateTime" use="required">
        <xs:annotation>
          <xs:documentation>
                        The date and time that this reservation was
                        created.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="HostCreateDate" type="xs:date" use="optional">
        <xs:annotation>
          <xs:documentation>
                        The date that this reservation was
                        created in the host system.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ModifiedDate" type="xs:dateTime" use="required">
        <xs:annotation>
          <xs:documentation>
                        The date and time that this reservation was last
                        modified for any reason.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Imported" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>Identifies a reservation that was originally created elsewhere and imported into a Universal Record.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="TicketingModifiersRef" type="common:typeRef" use="optional">
        <xs:annotation>
          <xs:documentation>
                        Reference to a Ticketing Modifers which is attached to this PNR. Ticketing Modifers referred  by this Key is a Primary Ticketing
                        Modifers. Worldspan Primary DI line will be supported using this feature.
               </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="InQueueMode" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>Identifies whether the gds pnr is being processed from the GDS queue.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="GroupRef" type="common:typeRef" use="optional">
        <xs:annotation>
          <xs:documentation>Represents a traveler group for Group booking and all their accompanying data. SUPPORTED PROVIDER: Worldspan and JAL. </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attributeGroup ref="common:attrElementKeyResults" />
      <xs:attribute name="OwningPCC" type="common:typePCC" use="optional">
        <xs:annotation>
          <xs:documentation>Indentifies the owning PCC of the PNR. PROVIDER SUPPORTED: Worldspan,JAL,Galileo and Apollo</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="providerReservation">
    <xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required" />
    <xs:attribute name="ProviderLocatorCode" type="common:typeProviderLocatorCode" use="required" />
  </xs:attributeGroup>
  <xs:attributeGroup name="optionalProviderReservation">
    <xs:annotation>
      <xs:documentation>
                Attribute group of optional provider and locator code.
                Use this group where provider and locator code both are
                optional
            </xs:documentation>
    </xs:annotation>
    <xs:attribute name="ProviderCode" type="common:typeProviderCode" use="optional" />
    <xs:attribute name="ProviderLocatorCode" type="common:typeProviderLocatorCode" use="optional" />
  </xs:attributeGroup>
  <xs:element name="ProviderReservationStatus">
    <xs:annotation>
      <xs:documentation>
                Status of the cancellation for this provider
                reservation.
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="CancelInfo" type="common:typeResultMessage">
          <xs:annotation>
            <xs:documentation>
                            If the provider reservation was not
                            successfully cancelled or cancelled with
                            warnings the provider system might provides
                            some textual information describing the
                            reason.
                        </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="CreateDate" type="xs:dateTime" use="required">
        <xs:annotation>
          <xs:documentation>
                        The date and time that this reservation was
                        created.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ModifiedDate" type="xs:dateTime" use="required">
        <xs:annotation>
          <xs:documentation>
                        The date and time that this reservation was last
                        modified for any reason.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ProviderCode" type="common:typeProviderCode" use="required">
        <xs:annotation>
          <xs:documentation>
                        Contains the Provider Code of the entity housing
                        the actual reservation in the event this is a
                        passive one.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="LocatorCode" type="common:typeProviderLocatorCode" use="required">
        <xs:annotation>
          <xs:documentation>
                        Contains the Locator Code of the actual
                        reservation in the event this is a passive
                        reservation.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Cancelled" type="xs:boolean" use="required">
        <xs:annotation>
          <xs:documentation>
                        Will be true if the reservation was successfuly
                        cancelled on the provider system.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirReservationCriteria">
    <xs:annotation>
      <xs:documentation>
                Criteria for searching the Air reservations
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="DepartureDate" type="typeDateSpec">
          <xs:annotation>
            <xs:documentation>Flight Departure Date or Date Range</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="ArrivalDate" type="typeDateSpec">
          <xs:annotation>
            <xs:documentation>Flight Arrival Date or Date Range</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="Origin" type="common:typeIATACode" use="optional">
        <xs:annotation>
          <xs:documentation>
                        The IATA location code for this origination of this entity.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Destination" type="common:typeIATACode" use="optional">
        <xs:annotation>
          <xs:documentation>
                        The IATA location code for this destination of this entity.
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="FlightNumber" type="xs:string" use="optional" />
      <xs:attribute name="Carrier" type="common:typeCarrier" use="optional" />
      <xs:attribute default="false" name="PassiveOnly" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>Search for Passives Only</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="HotelReservationCriteria">
    <xs:annotation>
      <xs:documentation>
                Criteria for searching the Hotel reservations
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="CheckInDate" type="typeDateSpec">
          <xs:annotation>
            <xs:documentation>Hotel Check In Date or Date Range</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="HotelChainCode" type="common:typeHotelChainCode">
        <xs:annotation>
          <xs:documentation>Hotel Chain Code</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="HotelCode" type="common:typeHotelCode" />
      <xs:attribute name="HotelConfirmation" type="xs:string" use="optional" />
      <xs:attribute name="Location" type="common:typeIATACode" />
      <xs:attribute default="false" name="PassiveOnly" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>Search for Passives Only</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="VehicleReservationCriteria">
    <xs:annotation>
      <xs:documentation>
                Criteria for searching the Vehicle reservations
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="PickUpDate" type="typeDateSpec">
          <xs:annotation>
            <xs:documentation>Vehicle PickUp Date or Date Range</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="VehicleConfirmation" type="xs:string" use="optional" />
      <xs:attribute name="Location" type="common:typeIATACode" />
      <xs:attribute name="VendorCode" type="common:typeSupplierCode">
        <xs:annotation>
          <xs:documentation>Vehicle Vendor Code</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="LocationNumber" type="xs:string" />
      <xs:attribute default="false" name="PassiveOnly" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>Search for Passives Only</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="RailReservationCriteria">
    <xs:annotation>
      <xs:documentation> Criteria for searching the Rail reservations
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="JourneyDepartureDate" type="typeDateSpec">
          <xs:annotation>
            <xs:documentation>Hotel Check In Date or Date Range</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="JourneyArrivalDate" type="typeDateSpec">
          <xs:annotation>
            <xs:documentation>Hotel Check In Date or Date Range</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="SegmentDepartureDate" type="typeDateSpec">
          <xs:annotation>
            <xs:documentation>Hotel Check In Date or Date Range</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="SegmentArrivalDate" type="typeDateSpec">
          <xs:annotation>
            <xs:documentation>Hotel Check In Date or Date Range</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="JourneyOrigin" type="common:typeIATACode" use="optional">
        <xs:annotation>
          <xs:documentation>
                            The IATA location code for this origination of this entity.
                        </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="JourneyDestination" type="common:typeIATACode" use="optional">
        <xs:annotation>
          <xs:documentation>
                            The IATA location code for this destination of this entity.
                        </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="JourneyRailLocOrigin" type="common:typeRailLocationCode" use="optional">
        <xs:annotation>
          <xs:documentation>
                            RCH specific origin code (a.k.a UCodes) which uniquely identifies a train station.
                        </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="JourneyRailLocDestination" type="common:typeRailLocationCode" use="optional">
        <xs:annotation>
          <xs:documentation>
                          RCH specific destination code (a.k.a UCodes) which uniquely identifies a train station.
                        </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SupplierCode" type="xs:string" use="optional" />
      <xs:attribute name="TrainNumber" type="xs:string" use="optional" />
      <xs:attribute default="false" name="PassiveOnly" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>Search for Passives Only</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="ProviderReservationSearchResult">
    <xs:annotation>
      <xs:documentation>
                Container for reservations that match the search
                criteria
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="common:Name" />
        <xs:element minOccurs="0" maxOccurs="unbounded" name="ProductInfo" type="typeProductInfo" />
        <xs:element minOccurs="0" ref="common:AgencyInfo" />
      </xs:sequence>
      <xs:attribute name="UniversalRecordLocatorCode" type="xs:string" use="optional" />
      <xs:attribute name="CreatedDate" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>
                        The date the resevation record was created
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="EarliestTravelDate" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>
                        The date on which th earliest travel can occur
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Ticketed" type="typeReservationTicketed" use="required">
        <xs:annotation>
          <xs:documentation>
                        If the reservation is ticketed. Applies to an
                        AIR reservation only
                    </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ProviderCode" type="xs:string" use="required" />
      <xs:attribute name="ProviderLocatorCode" type="xs:string" use="optional" />
      <xs:attribute name="ExternalSearchIndex" type="typeExternalSearchIndex" />
    </xs:complexType>
  </xs:element>
  <xs:element name="RecordIdentifier">
    <xs:annotation>
      <xs:documentation>
                The information that uniquly identifies a particular
                supplier reservation.
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="UniversalLocatorCode" type="common:typeLocatorCode" use="required" />
      <xs:attribute name="ProviderCode" type="common:typeProviderCode" use="optional" />
      <xs:attribute name="ProviderLocatorCode" type="common:typeLocatorCode" use="optional" />
    </xs:complexType>
  </xs:element>
  <xs:element name="UniversalModifyCmd">
    <xs:annotation>
      <xs:documentation>
                Container for the elements that will be batch updated to
                a UniversalRecord
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice>
        <xs:element minOccurs="0" ref="VehicleAdd" />
        <xs:element minOccurs="0" ref="VehicleDelete" />
        <xs:element minOccurs="0" ref="VehicleUpdate" />
        <xs:element minOccurs="0" ref="AirAdd" />
        <xs:element minOccurs="0" ref="AirDelete" />
        <xs:element minOccurs="0" ref="AirUpdate" />
        <xs:element minOccurs="0" ref="UniversalAdd" />
        <xs:element minOccurs="0" ref="UniversalDelete" />
        <xs:element minOccurs="0" ref="UniversalUpdate" />
        <xs:element minOccurs="0" ref="HotelAdd" />
        <xs:element minOccurs="0" ref="HotelUpdate" />
